{
    "name": "ekreative/health-check-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "A bundle that provides a simple /healthcheck route",
    "keywords": ["healthcheck", "doctrine", "redis"],
    "autoload": {
        "psr-4": {
            "Ekreative\\HealthCheckBundle\\": "src/Ekreative/HealthCheckBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekreative\\HealthCheckBundle\\": "tests/Ekreative/HealthCheckBundle/"
        },
        "classmap": [
            "tests/TestKernel.php"
        ]
    },
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^5.0|^6.0|^7.0"
    },
    "suggest": {
        "ext-redis": "To use the Redis health check"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0|^6.0|^7.0",
        "doctrine/doctrine-bundle": "^2",
        "phpunit/phpunit": "^11",
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/monolog-bundle": "^3.0|^4.0",
        "symfony/browser-kit": "^5.0|^6.0|^7.0",
        "symfony/yaml": "^5.0|^6.0|^7.0"
    },
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
