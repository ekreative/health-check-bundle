{
    "name": "ekreative/health-check-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "A bundle that provides a simple /healthcheck route",
    "keywords": ["healthcheck", "doctrine", "redis"],
    "autoload": {
        "psr-4": {
            "Ekreative\\HealthCheckBundle\\": "src/Ekreative/HealthCheckBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekreative\\HealthCheckBundle\\": "tests/Ekreative/HealthCheckBundle/"
        },
        "classmap": [
            "tests/TestKernel.php"
        ]
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "^3.4|^4.0"
    },
    "require-dev": {
        "ext-redis": "*",
        "symfony/phpunit-bridge": "^3.0",
        "doctrine/doctrine-bundle": "^1.6",
        "phpunit/phpunit": "^4.8|^5.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/browser-kit": "^3.0|^4.0"
    },
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ]
}
