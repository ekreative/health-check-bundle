{
    "name": "ekreative/health-check-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "A bundle that provides a simple /healthcheck route",
    "keywords": ["healthcheck", "doctrine", "redis"],
    "autoload": {
        "psr-4": {
            "Ekreative\\HealthCheckBundle\\": "src/Ekreative/HealthCheckBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekreative\\HealthCheckBundle\\": "tests/Ekreative/HealthCheckBundle/"
        },
        "classmap": [
            "tests/TestKernel.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "^3.4|^4.0|^5.0"
    },
    "require-dev": {
        "ext-redis": "*",
        "symfony/phpunit-bridge": "^3.4|^4.0|^5.0",
        "doctrine/doctrine-bundle": "^1.10|^2.0",
        "phpunit/phpunit": "^7",
        "friendsofphp/php-cs-fixer": "^2.14",
        "symfony/monolog-bundle": "^3.3",
        "symfony/browser-kit": "^3.4|^4.0|^5.0",
        "symfony/yaml": "^3.4|^4.0|^5.0"
    },
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ]
}
